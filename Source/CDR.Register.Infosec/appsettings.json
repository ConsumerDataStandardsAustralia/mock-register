{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Url": "https://0.0.0.0:7002",
        "Certificate": {
          "Path": "Certificates/dsb-server.pfx",
          "Password": "password"
        }
      }
    }
  },
  "SigningCertificate": {
    "Path": "Certificates/dsb-server",
    "Password": "password"
  },

  "Certificate": {
    "CertThumbprintNameHttpHeaderName": "X-TlsClientCertThumbprint",
    "CertCommonNameHttpHeaderName": "X-TlsClientCertCN"
  },
  "AccessTokenExpiryInSeconds": 300,
  "exclude": [
    "**/bin",
    "**/bower_components",
    "**/jspm_packages",
    "**/node_modules",
    "**/obj",
    "**/platforms"
  ]
}